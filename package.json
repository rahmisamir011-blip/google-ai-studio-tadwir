{
  "name": "tadwir",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "build": "esbuild index.tsx --bundle --minify --outfile=dist/index.js --loader:.tsx=tsx --define:process.env.API_KEY='\"'$API_KEY'\"' --define:process.env.FIREBASE_API_KEY='\"'$FIREBASE_API_KEY'\"' --define:process.env.FIREBASE_AUTH_DOMAIN='\"'$FIREBASE_AUTH_DOMAIN'\"' --define:process.env.FIREBASE_PROJECT_ID='\"'$FIREBASE_PROJECT_ID'\"' --define:process.env.FIREBASE_STORAGE_BUCKET='\"'$FIREBASE_STORAGE_BUCKET'\"' --define:process.env.FIREBASE_MESSAGING_SENDER_ID='\"'$FIREBASE_MESSAGING_SENDER_ID'\"' --define:process.env.FIREBASE_APP_ID='\"'$FIREBASE_APP_ID'\"' --define:process.env.FIREBASE_MEASUREMENT_ID='\"'$FIREBASE_MEASUREMENT_ID'\"' && cp index.html dist/index.html"
  },
  "dependencies": {
    "@google/genai": "^1.19.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "firebase": "^10.12.2"
  },
  "devDependencies": {
    "esbuild": "^0.23.0"
  }
}